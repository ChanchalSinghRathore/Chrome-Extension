{
  "manifest_version": 3,
  "name": "Smart Tab Organizer",
  "version": "1.0",
  "description": "Automatically organizes and groups tabs by domain, and enables a focus mode.",
  "permissions": ["tabs", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png"
  }
}
  